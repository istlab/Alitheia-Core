<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.metrics.mde.db.MDEDeveloper" table="MDE_DEVELOPER">
    <id name="id" column="MDE_DEVELOPER_ID">
      <generator class="native" />
    </id>
    <many-to-one name="developer" column="DEVELOPER_ID" not-null="true" cascade="merge" />
    <property name="start" column="START_TIME" not-null="true" type="java.util.Date" />
    <property name="startWeek" column="START_WEEK" not-null="true" />
    <property name="activeWeek" column="ACTIVE_WEEK" not-null="true" />
    <property name="serviceTime" column="SERVICE_TIME" not-null="true" />
  </class>
</hibernate-mapping>
