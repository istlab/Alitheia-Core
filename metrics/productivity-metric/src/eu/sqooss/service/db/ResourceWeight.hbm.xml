<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.ResourceWeight" table="PRODUCTIVITY_RESOURCE_WEIGHT">
    <id name="id" column="RESOURCE_WEIGHT_ID">
      <generator class="native" />
    </id>
    <property name="resourceTypeID" column="RESOURCE_TYPE_ID" />
    <property name="projectID" column="PROJECT_ID" />
    <property name="weight" column="WEIGHT" />
  </class>
</hibernate-mapping>
