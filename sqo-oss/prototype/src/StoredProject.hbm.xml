<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="eu.sqooss.db">
  <class name="StoredProject" table="STORED_PROJECT">
    <id name="id" column="ID">
      <generator class="native"/>
    </id>
    <set name="versions"
	 cascade="all">
	 <key column="project_id" not-null="true"/>
	 <one-to-many class="ProjectVersion" />
    </set>
    <property name="name" column="NAME"/>
    <property name="website" column="WEB_SITE"/>
    <property name="contactPoint" column="CONTACT_POINT"/>
    <property name="localPath" column="LOCAL_PATH"/>
    <property name="remotePath" column="REMOTE_PATH"/>
    <property name="svnUrl" column="SVN_URL"/>
    <property name="mailPath" column="MAIL_PATH"/>
  </class>
</hibernate-mapping>