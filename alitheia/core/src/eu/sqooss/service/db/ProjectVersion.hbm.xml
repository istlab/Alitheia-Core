<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.ProjectVersion" table="PROJECT_VERSION">
    <id name="id" column="PROJECT_VERSION_ID">
      <generator class="native" />
    </id>
    <many-to-one name="project" column="STORED_PROJECT_ID" not-null="true"/>
    <property name="version" column="VERSION" />
  </class>
</hibernate-mapping>
