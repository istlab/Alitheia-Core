<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.PrivilegeValue" table="PRIVILEGE_VALUE">
    <id name="id" column="PRIVILEGE_VALUE_ID">
      <generator class="native" />
    </id>
    <property name="value" column="VALUE" />
    <many-to-one name="privilege" column="PRIVILEGE_ID" not-null="true"/>
  </class>
</hibernate-mapping>

