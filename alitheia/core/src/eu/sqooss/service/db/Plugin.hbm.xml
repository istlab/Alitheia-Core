<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.Plugin" table="PLUGIN">
    <id name="id" column="PLUGIN_ID">
      <generator class="native" />
    </id>
    <property name="metric" column="METRIC_ID" />
    <property name="name" column="NAME" />
    <property name="installdate" column="INSTALL_DATE" type="java.util.Date" />
  </class>
</hibernate-mapping>
