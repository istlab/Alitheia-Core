<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.Sender" table="SENDER">
    <id name="id" column="SENDER_ID">
      <generator class="native" />
    </id>
    <property name="name" column="SENDER_NAME" />
    <property name="email" column="SENDER_EMAIL" />
  </class>
</hibernate-mapping>
