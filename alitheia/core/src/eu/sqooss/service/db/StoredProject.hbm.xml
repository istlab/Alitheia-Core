<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.StoredProject" table="STORED_PROJECT">
    <id name="id" column="PROJECT_ID">
      <generator class="native" />
    </id>
    <property name="name" column="PROJECT_NAME" />
    <property name="website" column="WEBSITE_URL" />
    <property name="contact" column="CONTACT_URL" />
    <property name="bugs" column="BTS_URL" />
    <property name="repository" column="SCM_URL" />
    <property name="mail" column="MAIL_URL" />
  </class>
</hibernate-mapping>
