<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.ProjectFileMeasurement" table="PROJECT_FILE_MEASUREMENT">
    <id name="id" column="PROJECT_FILE_MEASUREMENT_ID">
      <generator class="native" />
    </id>
    <many-to-one name="metric" column="METRIC_ID" not-null="true" cascade="merge" unique-key="unqmf"/>
    <many-to-one name="projectFile" column="PROJECT_FILE_ID" not-null="true" cascade="merge" unique-key="unqmf"/>
    <property name="result" column="RESULT" type="string" />
  </class>
</hibernate-mapping>
